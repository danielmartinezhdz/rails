<%title ||= "Crear"%>
<h1><%=title%></h1>
<%= form_for(@member) do |f|%>
	<div>
		<%=f.text_field :name, placeholder: "nombre" %>
	</div>
	<div>
		<%=f.text_field :lastname, placeholder: "Apellido" %>
	</div>
	<div>
		<%=f.text_field :age, placeholder: "Edad" %>
	</div>
	<div>
		<%=f.text_field :email, placeholder: "Email" %>
	</div>
	<div>
		<%=f.text_field :gender, placeholder: "Genero" %>
	</div>
	<div>
		<%=f.submit "Guardar", class: "btn btn-success" %>
	</div>
<%end%>